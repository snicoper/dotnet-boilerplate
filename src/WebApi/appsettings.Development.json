{
  "UseInMemoryDatabase": true,
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=JsonTaylorDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "IdentityServer": {
    "Key": {
      "Type": "Development"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Information",
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    }
  }
}
